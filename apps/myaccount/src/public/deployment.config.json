{
    "appBaseName": "myaccount",
    "appBaseNameForHistoryAPI": "/",
    "clientID": "MY_ACCOUNT",
    "debug": false,
    "idpConfigs": {
        "enablePKCE": true,
        "clockTolerance": 300,
        "responseMode": "query",
        "scope": ["SYSTEM"],
        "serverOrigin": "https://auth-sys.elveris.com",
        "storage": "webWorker",
        "authorizeEndpointURL": "${serverOrigin}/${tenantPrefix}/${superTenantDomain}/oauth2/authorize?ut=${userTenantDomain}",
        "logoutEndpointURL": "${serverOrigin}/${tenantPrefix}/${superTenantDomain}/oidc/logout",
        "oidcSessionIFrameEndpointURL": "${serverOrigin}/${tenantPrefix}/${superTenantDomain}/oidc/checksession"
    },
    "i18nResourcePath": "",
    "isSaas": true,
    "loginCallbackPath": "",
    "logoutCallbackPath": "",
    "serverOrigin": "https://auth-sys.elveris.com",
    "routePaths": {
        "home": "/overview",
        "login": "",
        "logout": "/logout"
    },
    "session": {
        "userIdleTimeOut": 600,
        "userIdleWarningTimeOut": 580,
        "sessionRefreshTimeOut": 300,
        "checkSessionInterval": 3
    },
    "tenantResolutionStrategy": "id_token",
    "ui": {
        "appCopyright": "Elveriş Identity Server ${copyright} ${year}",
        "appTitle": "My Account | Elveriş Identity Server",
        "appName": "My Account",
        "appLogoPath": "/assets/images/branding/logo.svg",
        "authenticatorApp": [],
        "brandingStoreURL": "${host}/extensions/branding/${tenantDomain}/branding-preference_${locale}.json",
        "disableMFAforSuperTenantUser": false,
        "enableMFAUserWise": false,
        "disableMFAForFederatedUsers": false,
        "forceBackupCode": false,
        "features": {
            "applications": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            },
            "overview": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            },
            "personalInfo": {
                "disabledFeatures": ["profileInfo.mobileVerification"],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            },
            "security": {
                "disabledFeatures": ["security.loginVerifyData.typingDNA"],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            }
        },
        "i18nConfigs": {
            "showLanguageSwitcher": true,
            "langAutoDetectEnabled": false
        },
        "isCookieConsentBannerEnabled": true,
        "isHeaderAvatarLabelAllowed": true,
        "privacyPolicyConfigs": {
            "visibleOnFooter": true
        },
        "productName": "Elveriş Identity Server",
        "productVersionConfig": {
            "allowSnapshot": true,
            "textCase": "uppercase",
            "labelColor": "primary"
        },
        "isProfileUsernameReadonly": false,
        "theme": {
            "name": "default"
        }
    }
}
