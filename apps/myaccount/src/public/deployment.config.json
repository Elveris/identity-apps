{
    "consoleApp": {
        "path": "/getting-started",
        "origin": "https://identity.elveris.com"
    },
    "appBaseName": "myaccount",
    "clientID": "MY_ACCOUNT",
    "loginCallbackPath": "",
    "logoutCallbackPath": "",
    "serverOrigin": "https://auth-sys.elveris.com",
    "routePaths": {
        "home": "/overview",
        "login": "",
        "logout": "/logout"
    },
    "extensions": {
        "applicationInsightsCookieDomain": "",
        "applicationInsightsCookiePostfix": "",
        "applicationInsightsEnabled": "true",
        "applicationInsightsInstrumentationKey": "",
        "applicationInsightsProxyEndpoint": ""
    },
    "superTenantProxy": "",
    "idpConfigs": {
        "logoutEndpointURL": "${serverOrigin}/${tenantPrefix}/${userTenantDomain}/oidc/logout",
        "clockTolerance": 300,
        "authorizeEndpointURL": "${serverOrigin}/${tenantPrefix}/${userTenantDomain}/oauth2/authorize",
        "oidcSessionIFrameEndpointURL": "${serverOrigin}/${tenantPrefix}/${userTenantDomain}/oidc/checksession",
        "serverOrigin": "https://auth-sys.elveris.com",
        "responseMode": "query"
    },
    "session": {
        "userIdleWarningTimeOut": 1740,
        "sessionRefreshTimeOut": 28800,
        "userIdleTimeOut": 1800,
        "checkSessionInterval": 5
    },
